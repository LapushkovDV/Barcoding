                             #WRITE_JSONERROR(JSONObj,'does not exist SaldoBox where nrec = ' + _tElements.value,'не найдена запись SaldoBox с nrec = ' + _tElements.value + '/позиция' +_npp)
                             mylog('не найдена запись SaldoBox с nrec = ' + _tElements.value);
                             result := JSONObj;
